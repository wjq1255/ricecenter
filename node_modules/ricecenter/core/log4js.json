{
  "appenders": [
    {
      "type": "dateFile",
      "absolute":true,
      "filename":"d:/temp/logs/",
      "pattern":"yyyy-MM-dd.log",
      "alwaysIncludePattern": true,
      "category": "access"
    },
    {
      "type": "file",
      "absolute":true,
      "filename":"d:/temp/logs/debug.log",
      "backups":10,
      "maxLogSize":10240000,
      "alwaysIncludePattern": true,
      "category": "debug"
    }
  ],
  "replaceConsole": true,
  "levels": {
    "access" : "INFO",
    "debug" : "DEBUG"
  }
}